<%= form_for(:ord, url:{:action=>'create'}) do |f|%>
<div class="field">
    <%= f.label :Name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :PhoneNumber %><br>
    <%= f.text_area :phonenum %>
  </div>
  <div class="form-group">
    <%= f.label :Address %><br>
    <%= f.text_field :address %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>